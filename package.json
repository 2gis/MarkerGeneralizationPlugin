{
  "name": "MarkerGeneralizationPlugin",
  "version": "1.0.0",
  "description": "Plugin for marker generalisation in 2gis maps and leaflet",
  "main": "dist/generalize.min.js",
  "scripts": {
    "build": "webpack -p",
    "build-dev": "webpack",
    "lint": "./node_modules/.bin/eslint ./src",
    "hook-install": "prepush install",
    "hook-remove": "prepush remove"
  },
  "prepush": {
    "tasks": [
      "npm run lint"
    ],
    "onDirtyState": "ask",
    "coloredOuput": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2gis/MarkerGeneralizationPlugin.git"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "prepush": "^3.1.11",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "@2gis/general": "0.3.0"
  }
}
